{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Overridable error",
  "type": "object",
  "properties": {
    "allOf": [{"$ref": "raml-util/schemas/error.schema"}],
    "overridableBlock": {
      "type": "object",
      "description": "Details of overridable block",
      "properties": {
        "name": {
          "type": "string",
          "description": "Name of the block",
          "enum": [
            "patronBlock",
            "itemLimitBlock",
            "itemNotLoanableBlock"
          ]
        },
        "missingPermissions": {
          "type": "array",
          "description": "Missing permissions required to override the block"
        }
      },
      "required": [
        "name",
        "missingPermissions"
      ]
    }
  },
  "required": [
    "overridableBlock"
  ]
}
